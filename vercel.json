{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/auth",
      "dest": "/api/index.js"
    },
    {
      "src": "/test-token",
      "dest": "/api/index.js"
    },
    {
      "src": "/commit",
      "dest": "/api/index.js"
    },
    {
      "src": "/create-pr",
      "dest": "/api/index.js"
    },
    {
      "src": "/merge-pr",
      "dest": "/api/index.js"
    },
    {
      "src": "/files",
      "dest": "/api/index.js"
    },
    {
      "src": "/file/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/prs",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/og",
      "dest": "/api/og.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
